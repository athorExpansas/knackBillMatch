# Knack Configuration
KNACK_APP_ID=your_app_id_here
KNACK_API_KEY=your_api_key_here

# Financial APIs
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret

# Bill.com API
BILLDOTCOM_API_KEY=your_billdotcom_api_key

# Bank Credentials (encrypted)
# Note: Use encrypt_credentials.py to generate encrypted values
BOFA_USERNAME=encrypted_username_here
BOFA_PASSWORD=encrypted_password_here
BOFA_ACCOUNT_NUMBER=1234  # Last 4 digits of account number

WELLS_FARGO_USERNAME=encrypted_username_here
WELLS_FARGO_PASSWORD=encrypted_password_here
WELLS_FARGO_ACCOUNT_NUMBER=5678  # Last 4 digits of account number

# Encryption Key (used by encrypt_credentials.py)
ENCRYPTION_KEY=your_encryption_key_here

# AWS Configuration for Nova Pro
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-west-2  # Region where Nova Pro is enabled

# Important Notes:
# 1. Never commit the actual .env file to version control
# 2. All passwords should be encrypted using encrypt_credentials.py
# 3. The AWS user must have permission to assume the BedrockAccessRole
# 4. Nova Pro requires an inference profile in the specified region
